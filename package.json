    "db:init": "npx tsx scripts/init-production-db.ts",
    "db:init:force": "npx tsx scripts/init-production-db.ts --force",
    "db:init:no-backup": "npx tsx scripts/init-production-db.ts --no-backup",
    "db:validate": "npx tsx scripts/init-production-db.ts --validate-only",
